<h1>Registrovaní uživatelé</h1>
<ul>
    {% for user in users %}
        <li>{{ user.email }}</li>
        <li>{{ user.name }}</li>
        <li>
            {% if user.role == 'ROLE_USER' %}
                Uživatel
            {% elseif user.role == 'ROLE_ADMIN' %}
                Administrátor
            {% else %}
                Neznámý stav
            {% endif %}
        </li>
        <hr />
    {% endfor %}
</ul>